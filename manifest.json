{
  "manifest_version": 2,
  "name": "WordReference",
  "version": "1.1",
  "description": "Translate selected word with WordReference",
  "icons": {
    "48": "icons/icono.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "tabs"
  ]
}
